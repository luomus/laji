{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "",
    "module": "commonjs",
    "types": [
      "node"
    ],
    "paths": {
      "@laji-map/*": ["app/shared-modules/laji-map-noop/*"],
      "@laji-form/*": ["app/shared-modules/laji-form-noop/*"]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ],
  "angularCompilerOptions": {
    "entryModule": "app/app.server.module#AppServerModule"
  },
  "include": [
    "**/*.ts"
  ]
}

    "laji-map": "^1.0.480",
    "leaflet.sync": "^0.2.4",
    "localforage": "^1.7.3",
    "moment": "2.22.2",
    "ngx-bootstrap": "3.0.1",
    "ngx-clipboard": "^11.1.9",
    "ngx-toastr": "^9.1.1",
    "ngx-webstorage": "3.0.2",
    "object-hash": "^1.3.1",
    "openseadragon": "^2.3.1",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "ts-loader": "5.3.1",
    "vm": "^0.1.0",
    "web-animations-js": "^2.3.1",
    "xlsx": "0.13.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "^7.1.0",
    "@angular/compiler-cli": "^7.1.1",
    "@angular/language-service": "^7.1.1",
    "@types/d3-axis": "^1.0.10",
    "@types/d3-format": "^1.3.0",
    "@types/d3-scale": "^2.0.2",
    "@types/d3-selection": "^1.3.4",

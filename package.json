    "@angular/router": "^4.0.0",
    "@ngx-translate/core": "^6.0.1",
    "@swimlane/ngx-datatable": "^7.3.0",
    "angular-tree-component": "3.2.4",
    "angular2-moment": "^1.3.3",
    "core-js": "^2.4.1",
    "deepmerge": "^1.3.2",
    "ie-shim": "^0.1.0",
    "intl": "^1.2.5",
    "laji-form": "0.0.170",

{
  "name": "laji-front",
  "version": "1.0.12",
  "description": "laji.fi frontend",
  "scripts": {
    "ng": "ng",
    "ng:version": "ng --version",
    "start": "ng serve --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000 --extract-css",
    "start:aot": "ng serve --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000 --aot --extract-css",
    "start:embedded": "ng serve --environment=embedded --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000 --extract-css",
    "start:vir": "ng serve --environment=vir --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000 --extract-css",
    "build": "ng build --prod --aot --extract-css",
    "build:embedded": "ng build --environment=embedded --prod --aot --extract-css",
    "build:vir": "ng build --environment=vir --prod --aot --extract-css",
    "build:dev": "ng build --environment=dev --prod --aot --extract-css",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "contributors": [
    "Ville-Matti Riihikoski <ville-matti.riihikoski@helsinki.fi>",
    "Kalle Myllymaa <kalle.myllymaa@helsinki.fi"

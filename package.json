{
  "name": "laji.fi-fron",
  "version": "1.0.0",
  "description": "laji.fi frontend",
  "scripts": {
    "typings-install": "typings install",
    "postinstall": "npm run typings-install",
    "build": "NODE_ENV=production node node_modules/.bin/webpack -p --config ./webpack.production.config.js && cp -R ./src/*  ./dist && rm -rf ./dist/app",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000 --content-base src",
    "start": "npm run server"
  },
  "contributors": [
    "Ville-Matti Riihikoski <ville-matti.riihikoski@helsinki.fi>",
    "Kalle Myllymaa"
  ],
  "license": "MIT",
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^1.1.1",
    "css-loader": "^0.23.1",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "to-string-loader": "^1.1.4",
    "typescript": "~1.8.9",
    "typings": "~1.3.2",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "^0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "angular2-localstorage": "^0.4.0",
    "core-js": "^2.2.0",
    "ie-shim": "^0.1.0",
    "ng2-bootstrap": "^1.0.24",
    "ng2-nvd3": "^1.1.3",
    "ng2-translate": "2.2.2",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.3.0",
    "react-addons-update": "^15.3.0",
    "react-dom": "^15.3.0",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "~0.6.12"
  },
  "keywords": [
    "laji.fi",
    "angular2"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org:luomus/laji.fi-front.git"
  },
  "homepage": "https://bitbucket.org/luomus/laji.fi-front"
}

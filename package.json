{
  "name": "laji-front",
  "version": "1.0.40",
  "description": "laji.fi frontend",
  "scripts": {
    "ng": "ng",
    "ng:version": "ng --version",
    "prepare": "patch-package",
    "start": "ng serve --configuration=local --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000",
    "start:local:prod": "NODE_ENV=development ng serve --aot=false --build-optimizer=false --configuration=local --prod --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000",
    "start:aot": "ng serve --configuration=local --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000",
    "start:prod": "ng serve --configuration=prod --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000",
    "start:embedded": "ng serve --configuration=embedded --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000",
    "start:vir": "ng serve --configuration=vir --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000",
    "build": "ng build --prod",
    "build:local": "ng build --configuration=local --prod",
    "build:embedded": "ng build --configuration=embedded --prod",
    "build:vir": "ng build --configuration=vir --prod",
    "build:dev": "ng build --configuration=dev --prod",
    "bundle-report": "ng build --prod --aot --stats-json --named-chunks=true && webpack-bundle-analyzer dist/stats.json",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "postinstall": "node ./scripts/remove-extras.js",
    "build:server:prod": "ng run laji:server && webpack --config webpack.server.config.js --progress --colors",
    "build:server:local": "ng run laji:server --configuration=local && webpack --config webpack.server.config.js --progress --colors",
    "build:ssr:prod": "yarn run build:server:prod && yarn run build",
    "build:ssr:local": "yarn run build:server:prod && yarn run build:local",
    "server": "node local.js"
  },
  "contributors": [
    "Ville-Matti Riihikoski <ville-matti.riihikoski@helsinki.fi>",
    "Kalle Myllymaa <kalle.myllymaa@helsinki.fi"
  ],
  "license": "MIT",
  "angular-cli": {},
  "private": false,

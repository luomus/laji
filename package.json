{
  "name": "laji.fi-front",
  "version": "1.0.0",
  "description": "laji.fi frontend",
  "scripts": {
    "typings-install": "typings install",
    "postinstall": "npm run typings-install",
    "build": "NODE_ENV=production node node_modules/.bin/webpack -p --define process.env.NODE_ENV='\"production\"' --config ./webpack.production.config.js && cp -R ./src/*  ./dist && rm -rf ./dist/app ./dist/*.ts ./dist/*.map",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --colors --progress --d --display-reasons --display-modules --display-error-details --display-cached --host '0.0.0.0' --port 3000 --content-base src",
    "start": "npm run server"
  },
  "contributors": [
    "Ville-Matti Riihikoski <ville-matti.riihikoski@helsinki.fi>",
    "Kalle Myllymaa <kalle.myllymaa@helsinki.fi"
  ],
  "license": "MIT",
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^1.1.1",
    "css-loader": "^0.23.1",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "to-string-loader": "^1.1.4",
    "typescript": "~2.0.2",
    "typings": "~1.3.2",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/platform-server": "2.0.0",
    "@angular/router": "3.0.0",
    "angular2-localstorage": "git+https://github.com/brennerdvaz/angular2-localstorage.git",
    "core-js": "^2.4.0",
    "ie-shim": "^0.1.0",
    "laji-form": "*",
    "laji-map": "*",
    "ng2-bootstrap": "1.1.5",
    "ng2-nvd3": "^1.1.3",
    "ng2-select": "^1.1.0",
    "ng2-translate": "2.4.3",
    "rxjs": "5.0.0-beta.11",
    "zone.js": "~0.6.17"
  },
  "keywords": [
    "laji.fi",
    "angular2"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org:luomus/laji.fi-front.git"
  },
  "homepage": "https://bitbucket.org/luomus/laji.fi-front"
}

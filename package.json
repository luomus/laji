{
  "name": "laji-front",
  "version": "1.0.36",
  "description": "laji.fi frontend",
  "scripts": {
    "ng": "ng",
    "ng:version": "ng --version",
    "start": "ng serve --environment=local --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000 --extract-css",
    "start:aot": "ng serve --environment=local --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000 --aot --extract-css",
    "start:prod": "ng serve --environment=prod --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000 --extract-css",
    "start:embedded": "ng serve --environment=embedded --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000 --extract-css",
    "start:vir": "ng serve --environment=vir --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000 --extract-css",
    "build": "ng build --prod --aot --extract-css --build-optimizer",
    "build:embedded": "ng build --environment=embedded --prod --aot --extract-css --build-optimizer",
    "build:vir": "ng build --environment=vir --prod --aot --extract-css --build-optimizer",
    "build:dev": "ng build --environment=dev --prod --aot --extract-css --build-optimizer",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "contributors": [
    "Ville-Matti Riihikoski <ville-matti.riihikoski@helsinki.fi>",
    "Kalle Myllymaa <kalle.myllymaa@helsinki.fi"
  ],
  "license": "MIT",

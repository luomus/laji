{
  "name": "laji-front",
  "version": "1.0.6",
  "description": "laji.fi frontend",
  "scripts": {
    "ng": "ng",
    "ng:versoin": "ng --version",
    "start": "ng serve --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000",
    "start:aot": "ng serve --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000 --aot",
    "start:embedded": "ng serve --environment=embedded --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000",
    "start:vir": "ng serve --environment=vir --proxy-config proxy.conf.js --host 0.0.0.0 --port 3000",
    "build": "ng build --prod --aot",
    "build:embedded": "ng build --environment=embedded --prod --aot",
    "build:vir": "ng build --environment=vir --prod --aot",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "contributors": [
    "Ville-Matti Riihikoski <ville-matti.riihikoski@helsinki.fi>",

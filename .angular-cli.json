{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "version": "1.0.0-beta.29",
    "name": "laji-front"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "static",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "laji",
      "styles": [
        "../node_modules/react-widgets/lib/less/react-widgets.less",
        "../node_modules/leaflet/dist/leaflet.css",
        "../node_modules/leaflet-draw/dist/leaflet.draw.css",
        "../node_modules/leaflet-contextmenu/dist/leaflet.contextmenu.css",
        "../node_modules/Leaflet.vector-markers/dist/leaflet-vector-markers.css",
        "../node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
        "../node_modules/leaflet.markercluster/dist/MarkerCluster.css",
        "../node_modules/laji-map/lib/lib/Leaflet.rrose/leaflet.rrose.css",
        "../node_modules/laji-map/lib/styles.css",
        "../node_modules/react-bootstrap-switch/dist/css/bootstrap3/react-bootstrap-switch.css",
        "../node_modules/react-spinner/react-spinner.css",
        "../node_modules/react-selectize/themes/index.css",
        "../node_modules/laji-form/lib/styles.css",
        "../node_modules/@swimlane/ngx-datatable/release/index.css",
        "../node_modules/@swimlane/ngx-datatable/release/themes/material.css",
        "../node_modules/@swimlane/ngx-datatable/release/assets/icons.css",
        "styles/ng2-toastr.min.css",
        "styles/laji.css"
      ],
      "scripts": [
        "../node_modules/moment/moment.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts",
        "embedded": "environments/environment.embedded.ts",
        "vir": "environments/environment.vir.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}

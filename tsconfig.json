{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "paths": {
      "@laji-map/*": [
        "app/shared-modules/laji-map/*"
      ],
      "@laji-form/*": [
        "app/shared-modules/laji-form/*"
      ],
      "generic-label-maker": [
        "dist/generic-label-maker"
      ],
      "generic-label-maker/*": [
        "dist/generic-label-maker/*"
      ]
    },
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom"
    ]
  }
}
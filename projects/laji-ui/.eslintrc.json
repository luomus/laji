{
  "extends": [
    "../../.eslintrc.json"
  ],
  "ignorePatterns": [
    "src/lib/typeahead/**/*.ts"
  ],
  "rules": {
    "@angular-eslint/component-selector": [
      "error",
      {
        "type": "element",
        "prefix": ["lu"],
        "style": "kebab-case"
      }
    ],
    "@angular-eslint/directive-selector": [
      "error",
      {
        "type": "attribute",
        "prefix": ["lu"],
        "style": "camelCase"
      }
    ]
  }
}
